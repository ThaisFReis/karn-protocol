{
  "name": "@karn_lat/protocol-sdk",
  "version": "0.1.0-alpha.5",
  "description": "Official TypeScript SDK for Karn Protocol - Contribution-driven governance on Stellar",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./clients": {
      "types": "./dist/clients/index.d.ts",
      "import": "./dist/clients/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./wallet": {
      "types": "./dist/wallet/index.d.ts",
      "import": "./dist/wallet/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "stellar",
    "soroban",
    "governance",
    "dao",
    "SBT"
  ],
  "author": "Karn Protocol",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@albedo-link/intent": "^0.13.0",
    "@stellar/freighter-api": "^1.7.0",
    "@stellar/stellar-sdk": "^14.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^25.2.1",
    "@types/react": "^18.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThaisFReis/karn-protocol.git"
  },
  "bugs": {
    "url": "https://github.com/ThaisFReis/karn-protocol/issues"
  },
  "homepage": "https://karn-protocol.vercel.app/"
}
